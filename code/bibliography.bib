	Number = {2},
	Pages = {514--526.e13},
	Publisher = {Elsevier BV},
	Title = {Competing Roles of Slow Oscillations and Delta Waves in Memory Consolidation versus Forgetting},
	Url = {http://dx.doi.org/10.1016/j.cell.2019.08.040},
	Volume = {179},
	Year = {2019},
