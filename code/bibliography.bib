	Publisher = {American Psychological Association (APA)},
	Tags = {replay},
	Title = {Reactivation of hippocampal ensemble activity patterns in the aging rat.},
	Url = {http://dx.doi.org/10.1037/0735-7044.115.6.1180},
	Volume = {115},
	Year = {2001},
