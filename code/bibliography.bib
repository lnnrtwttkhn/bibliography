	Journal = {bioRxiv},
	Publisher = {Cold Spring Harbor Laboratory},
	Title = {Hippocampus has lower oxygenation and weaker control of brain blood flow than cortex, due to microvascular differences},
	Url = {https://www.biorxiv.org/content/early/2019/11/08/835728},
	Year = {2019},
