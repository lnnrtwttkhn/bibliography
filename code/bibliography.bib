	Issn = {1364-6613},
	Journal = {Trends in Cognitive Sciences},
	Month = {Sep},
	Number = {9},
	Pages = {424--430},
	Publisher = {Elsevier BV},
	Tags = {MRI; methods},
	Title = {Beyond mind-reading: multi-voxel pattern analysis of {fMRI} data},
	Url = {http://dx.doi.org/10.1016/j.tics.2006.07.005},
	Volume = {10},
	Year = {2006},
