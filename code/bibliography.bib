	Issn = {0896-6273},
	Journal = {Neuron},
	Month = {Nov},
	Number = {4},
	Pages = {677--694},
	Publisher = {Elsevier BV},
	Title = {The Future of Memory: Remembering, Imagining, and the Brain},
	Url = {http://dx.doi.org/10.1016/j.neuron.2012.11.001},
	Volume = {76},
	Year = {2012},
