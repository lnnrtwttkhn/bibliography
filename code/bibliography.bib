	Publisher = {Springer US},
	Tags = {koenigsberg},
	Title = {{Hippocampal-neocortical interactions sharpen over time for predictive actions}},
	Url = {https://www.nature.com/articles/s41467-019-12016-9},
	Volume = {10},
	Year = {2019},
	Bdsk-File-1 = {}}

