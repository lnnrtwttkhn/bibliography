	Number = {1},
	Pages = {52--59},
	Publisher = {S. Karger AG},
	Tags = {replay; ripples; disorders},
	Title = {Disordered Ripples Are a Common Feature of Genetically Distinct Mouse Models Relevant to Schizophrenia},
	Url = {http://dx.doi.org/10.1159/000380765},
	Volume = {1},
	Year = {2015},
	Bdsk-File-1 = {}}

