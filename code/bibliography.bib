	Title = {Cued Memory Retrieval Exhibits Reinstatement of High Gamma Power on a Faster Timescale in the Left Temporal Lobe and Prefrontal Cortex},
	Url = {http://dx.doi.org/10.1523/JNEUROSCI.3810-16.2017},
