	Pages = {1073--1188},
	Publisher = {Wiley},
	Tags = {replay; review; ripples},
	Title = {Hippocampal sharp wave-ripple: A cognitive biomarker for episodic memory and planning},
	Url = {http://dx.doi.org/10.1002/hipo.22488},
	Volume = {25},
	Year = {2015},
