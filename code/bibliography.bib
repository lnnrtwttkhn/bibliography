	Keywords = {neural decoding, MVPA, RSA, hyperalignment, population response, fMRI},
	Month = {Jul},
	Number = {1},
	Pages = {435--456},
	Publisher = {Annual Reviews},
	Tags = {MVPA; review},
	Title = {Decoding Neural Representational Spaces Using Multivariate Pattern Analysis},
	Url = {http://dx.doi.org/10.1146/annurev-neuro-062012-170325},
	Volume = {37},
	Year = {2014},
