	Journal = {Current Biology},
	Month = {Sep},
	Number = {17},
	Pages = {1622--1627},
	Publisher = {Elsevier BV},
	Tags = {koenigsberg},
	Title = {{Shaping of object representations in the human medial temporal lobe based on temporal regularities}},
	Url = {http://dx.doi.org/10.1016/j.cub.2012.06.056},
	Volume = {22},
	Year = {2012},
	Bdsk-File-1 = {}}

