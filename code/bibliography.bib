	Number = {2},
	Pages = {296--301},
	Publisher = {Elsevier BV},
	Title = {Theta Phase-Coordinated Memory Reactivation Reoccurs in a Slow-Oscillatory Rhythm during NREM Sleep},
	Url = {http://dx.doi.org/10.1016/j.celrep.2018.09.037},
	Volume = {25},
