@article{Staresina2019TCS,
	Abstract = {Episodic memory allows us to mentally travel through time. How does the brain convert a simple reminder cue into a full-blown memory of past events and experiences? In this review, we integrate recent developments in the cognitive neuroscience of human memory retrieval, pinpointing the neural chronometry underlying successful recall. Electrophysiological recordings suggest that sensory cues proceed into the medial temporal lobe within the first 500 ms. At this point, a hippocampal process sets in, geared toward internal pattern completion and coordination of cortical memory reinstatement between 500 and 1500 ms. We further highlight the dynamic principles governing the recall process, which include a reversal of perceptual information flows, temporal compression, and theta clocking.},
	Author = {Staresina, Bernhard P. and Wimber, Maria},
	Date-Added = {2019-11-25 14:26:03 +0100},
	Date-Modified = {2019-11-27 10:55:00 +0100},
