	Issn = {1554-3528},
	Journal = {Behavior Research Methods},
	Keywords = {Novel objects, Unnameable stimuli, Picture database, Categorization, Novelty ratings, Word learning },
	Month = {Sep},
	Number = {4},
	Pages = {1393--1409},
	Publisher = {Springer Science and Business Media LLC},
	Tags = {koenigsberg},
	Title = {{The Novel Object and Unusual Name (NOUN) Database: A collection of novel images for use in experimental research}},
	Url = {http://dx.doi.org/10.3758/s13428-015-0647-3},
	Volume = {48},
