	Pages = {206--234},
	Publisher = {Elsevier BV},
	Title = {{The cave of shadows: Addressing the human factor with generalized additive mixed models}},
	Url = {http://dx.doi.org/10.1016/j.jml.2016.11.006},
	Volume = {94},
	Year = {2017},
