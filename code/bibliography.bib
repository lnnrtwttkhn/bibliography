	Journal = {Nature Neuroscience},
	Month = {Jun},
	Number = {7},
	Pages = {1032--1039},
	Publisher = {Springer Science and Business Media LLC},
	Title = {Segmentation of spatial experience by hippocampal theta sequences},
	Url = {http://dx.doi.org/10.1038/nn.3138},
