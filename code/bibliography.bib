	Number = {5853},
	Pages = {1147--1150},
	Publisher = {American Association for the Advancement of Science (AAAS)},
	Tags = {replay; PFC},
	Title = {Fast-Forward Playback of Recent Memory Sequences in Prefrontal Cortex During Sleep},
	Url = {http://dx.doi.org/10.1126/science.1148979},
