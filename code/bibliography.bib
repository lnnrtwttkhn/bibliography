	Journal = {arXiv e-prints},
	Keywords = {Computer Science; Machine Learning; Statistics; Machine Learning},
	Month = {Oct},
	Pages = {arXiv:1810.03880},
	Title = {Continual State Representation Learning for Reinforcement Learning using Generative Replay},
	Url = {https://ui.adsabs.harvard.edu/abs/2018arXiv181003880C},
	Year = {2018},
	Bdsk-File-1 = {}}

@article{Bradfield2020NBR,
	Abstract = {The orbitofrontal cortex (OFC) has been proposed to function as a cognitive map of task space: a mental model of the steps involved in a task. This idea has proven popular because it provides a cohesive explanation for a number of disparate findings regarding the OFC's role in a broad array of tasks. Concurrently, evidence has begun to reveal the functional heterogeneity of OFC subregions, particularly the medial and lateral OFC. How these subregions uniquely contribute to the OFC's role as a cognitive map of task space, however, has not been explored. Here we propose that, in rodents, the lateral OFC represents the agent's initial position within that task map (i.e. initial state), determining which actions are available as a consequence of that position, whereas the medial OFC represents the agent's future position within the task map (i.e. terminal state), influencing which actions are selected to achieve that position. We argue that these processes are achieved somewhat independently and somewhat interdependently, and are achieved through similar but non-identical circuitry.},
	Author = {Bradfield, Laura A. and Hart, Genevra},
	Date-Added = {2019-11-29 09:39:38 +0100},
	Date-Modified = {2019-11-29 09:40:53 +0100},
	Doi = {10.1016/j.neubiorev.2019.11.009},
	Issn = {0149-7634},
	Journal = {Neuroscience & Biobehavioral Reviews},
	Keywords = {Medial orbitofrontal cortex; Lateral orbitofrontal cortex; Decision-making; Cognitive map of task space; Goal-directed action; Model-based reinforcement learning},
	Month = {Jan},
	Pages = {287--294},
	Publisher = {Elsevier BV},
	Title = {Rodent medial and lateral orbitofrontal cortices represent unique components of cognitive maps of task space},
	Url = {http://dx.doi.org/10.1016/j.neubiorev.2019.11.009},
	Volume = {108},
	Year = {2020},
	Bdsk-File-1 = {}}

@article{Nadel2012NBR,
	Abstract = {Memory formation is a highly dynamic process. In this review we discuss traditional views of memory and offer some ideas about the nature of memory formation and transformation. We argue that memory traces are transformed over time in a number of ways, but that understanding these transformations requires careful analysis of the various representations and linkages that result from an experience. These transformations can involve: (1) the selective strengthening of only some, but not all, traces as a function of synaptic rescaling, or some other process that can result in selective survival of some traces; (2) the integration (or assimilation) of new information into existing knowledge stores; (3) the establishment of new linkages within existing knowledge stores; and (4) the up-dating of an existing episodic memory. We relate these ideas to our own work on reconsolidation to provide some grounding to our speculations that we hope will spark some new thinking in an area that is in need of transformation.},
	Author = {Nadel, L. and Hupbach, A. and Gomez, R. and Newman-Smith, K.},
	Date-Added = {2019-11-29 09:32:25 +0100},
	Date-Modified = {2019-11-29 09:33:32 +0100},
	Doi = {10.1016/j.neubiorev.2012.03.001},
	Issn = {0149-7634},
	Journal = {Neuroscience & Biobehavioral Reviews},
	Month = {Aug},
	Number = {7},
	Pages = {1640--1645},
	Publisher = {Elsevier BV},
	Tags = {Memory consolidation; Transformation; Reconsolidation},
	Title = {Memory formation, consolidation and transformation},
	Url = {http://dx.doi.org/10.1016/j.neubiorev.2012.03.001},
	Volume = {36},
