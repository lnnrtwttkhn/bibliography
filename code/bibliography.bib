	Publisher = {Elsevier BV},
	Tags = {fmriprep},
	Title = {A component based noise correction method (CompCor) for BOLD and perfusion based fMRI},
	Url = {http://dx.doi.org/10.1016/j.neuroimage.2007.04.042},
	Volume = {37},
	Year = {2007},
