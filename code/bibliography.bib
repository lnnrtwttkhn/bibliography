	Publisher = {Springer Science and Business Media LLC},
	Tags = {replay, review},
	Title = {The hippocampal sharp wave--ripple in memory retrieval for immediate use and consolidation},
	Url = {http://dx.doi.org/10.1038/s41583-018-0077-1},
