	Month = {Feb},
	Number = {10},
	Pages = {E962--E971},
	Publisher = {Proceedings of the National Academy of Sciences},
	Title = {Task context impacts visual object processing differentially across the cortex},
	Url = {http://dx.doi.org/10.1073/pnas.1312567111},
	Volume = {111},
	Year = {2014},
