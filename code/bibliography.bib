	Journal = {NeuroImage},
	Month = {Nov},
	Pages = {677--682},
	Publisher = {Elsevier BV},
	Tags = {open science},
	Title = {Why share data? Lessons learned from the {fMRIDC}},
	Url = {http://dx.doi.org/10.1016/j.neuroimage.2012.11.010},
	Volume = {82},
	Year = {2013},
