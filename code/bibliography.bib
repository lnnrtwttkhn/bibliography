	Keywords = {Mixed models, Random effects, Stimulus sampling},
	Number = {1},
	Pages = {54--69},
	Tags = {highspeed},
	Title = {{Treating stimuli as a random factor in social psychology: A new and comprehensive solution to a pervasive but largely ignored problem}},
