	Journal = {bioRxiv},
	Publisher = {Cold Spring Harbor Laboratory},
	Title = {The Tolman-Eichenbaum Machine: Unifying space and relational memory through generalisation in the hippocampal formation},
	Year = {2019},
	Bdsk-File-1 = {}}

