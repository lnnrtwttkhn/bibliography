	Pages = {1055--1058},
	Publisher = {American Association for the Advancement of Science (AAAS)},
	Tags = {place cells},
	Title = {Dynamics of the hippocampal ensemble code for space},
	Url = {http://dx.doi.org/10.1126/science.8351520},
	Volume = {261},
	Year = {1993},
