	Tags = {replay; review},
	Title = {Abnormal Sleep Spindles, Memory Consolidation, and Schizophrenia},
	Url = {http://dx.doi.org/10.1146/annurev-clinpsy-050718-095754},
	Volume = {15},
	Year = {2019},
