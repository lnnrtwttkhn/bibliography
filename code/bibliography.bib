@article{Ruch2020TCS,
	Abstract = {Can information that is processed during sleep influence awake behavior? Recent research demonstrates that learning during sleep is possible, but that sleep-learning invariably produces memory traces that are consciously inaccessible in the awake state. Thus, sleep-learning can likely exert implicit, but not explicit, influences on awake behavior.},
