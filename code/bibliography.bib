	Publisher = {Proceedings of the National Academy of Sciences},
	Title = {Navigation-related structural change in the hippocampi of taxi drivers},
	Url = {http://dx.doi.org/10.1073/pnas.070039597},
	Volume = {97},
	Year = {2000},
