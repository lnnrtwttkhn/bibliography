	Keywords = {long-term memory; cortical learning; engrams; hippocampus},
	Month = {Dec},
	Number = {12},
	Pages = {989--1002},
	Publisher = {Elsevier BV},
	Tags = {review},
	Title = {Rapid Cortical Plasticity Supports Long-Term Memory Formation},
