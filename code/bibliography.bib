	Journal = {bioRxiv},
	Publisher = {Cold Spring Harbor Laboratory},
	Title = {Analysis of task-based functional {MRI} data preprocessed with {fMRIPrep}},
	Url = {https://www.biorxiv.org/content/early/2019/07/08/694364},
	Year = {2019},
