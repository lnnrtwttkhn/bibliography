	Month = {Nov},
	Number = {48},
	Pages = {19591--19596},
	Publisher = {Proceedings of the National Academy of Sciences},
	Title = {Persistence of hippocampal multivoxel patterns into postencoding rest is related to memory},
	Url = {http://dx.doi.org/10.1073/pnas.1308499110},
	Volume = {110},
	Year = {2013},
