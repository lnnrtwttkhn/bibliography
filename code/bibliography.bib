	Publisher = {Springer Science and Business Media LLC},
	Title = {Novelist {Cormac McCarthy's} tips on how to write a great science paper},
	Url = {http://dx.doi.org/10.1038/d41586-019-02918-5},
	Volume = {574},
	Year = {2019},
