	Publisher = {SAGE Publications},
	Title = {Revisiting {Snodgrass} and {Vanderwart's} Object Pictorial Set: The Role of Surface Detail in Basic-Level Object Recognition},
	Url = {http://dx.doi.org/10.1068/p5117},
	Volume = {33},
