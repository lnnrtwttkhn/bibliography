	Journal = {bioRxiv},
	Publisher = {Cold Spring Harbor Laboratory},
	Title = {Hippocampal ensembles represent sequential relationships among discrete nonspatial events},
	Url = {https://www.biorxiv.org/content/early/2019/11/13/840199},
	Year = {2019},
