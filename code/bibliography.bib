	Issn = {2398-502X},
	Journal = {Wellcome Open Research},
	Month = {Apr},
	Pages = {63},
	Publisher = {F1000 (Faculty of 1000 Ltd)},
	Title = {{Raincloud plots: A multi-platform tool for robust data visualization}},
	Url = {http://dx.doi.org/10.12688/wellcomeopenres.15191.1},
