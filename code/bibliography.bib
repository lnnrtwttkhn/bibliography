	Publisher = {Society for Neuroscience},
	Tags = {MRI},
	Title = {Linear Systems Analysis of Functional Magnetic Resonance Imaging in Human {V1}},
	Url = {http://dx.doi.org/10.1523/JNEUROSCI.16-13-04207.1996},
	Volume = {16},
	Year = {1996},
