	Journal = {Hippocampus},
	Keywords = {appetitive, aversive, consolidation, emotion, hippocampus, memory, reactivation, replay, sharp-waves ripples, sleep},
	Number = {1},
	Pages = {19-27},
	Title = {Validating the theoretical bases of sleep reactivation during sharp-wave ripples and their association with emotional valence},
	Url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/hipo.23143},
	Volume = {30},
