	Issn = {1364-6613},
	Journal = {Trends in Cognitive Sciences},
	Keywords = {memory; recall; hippocampus; pattern completion; reinstatement; theta},
	Month = {Dec},
	Number = {12},
	Pages = {1071--1085},
	Publisher = {Elsevier BV},
	Tags = {replay; review},
	Title = {A Neural Chronometry of Memory Recall},
