	Issn = {1095-9203},
	Journal = {Science},
	Month = {Jan},
	Number = {6473},
	Pages = {eaaw4325},
	Publisher = {American Association for the Advancement of Science (AAAS)},
	Tags = {memory},
	Title = {Memory engrams: Recalling the past and imagining the future},
	Url = {http://dx.doi.org/10.1126/science.aaw4325},
	Volume = {367},
	Year = {2020},
	Bdsk-File-1 = {}}

